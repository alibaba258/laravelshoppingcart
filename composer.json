{
    "name": "alibaba258/cart",
    "description": "Laravel 8 Shopping cart",
    "keywords": ["laravel", "shopping cart", "cart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Alberti",
            "email": "me@marcel-alberti.de"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0",
        "illuminate/validation": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0",
        "illuminate/translation": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0.0",
        "symfony/var-dumper": "2.7.*@dev"
    },
    "autoload": {
        "psr-4": {
            "alibaba258\\": "src/alibaba258"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "alibaba258\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "alibaba258\\Cart\\Facades\\CartFacade"
            }
        }
    }
}
